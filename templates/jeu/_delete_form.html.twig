<form method="post" action="{{ path('app_jeu_delete', {'id': jeu.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer ce jeu ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ jeu.id) }}">
    <button class="btn error btn-delete shadow-1 primary rounded-4 btn-large primary">Supprimer</button>
</form>
